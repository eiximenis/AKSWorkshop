@model BeerBook.Models.Responses.PagedResponse<BeerBook.Models.Responses.OrderListItem>
@{
    ViewData["Title"] = "List";
}

<h1>Your orders</h1>


<table class="table">
    <tr>
        <th>Purchased By</th>
        <th>Purchased At</th>
        <th># Beers</th>
    </tr>
    @foreach (var order in Model.Data)
    {
        <tr>
            <td>@order.UserName</td>
            <td>@order.PurchasedAt</td>
            <td>@order.TotalBeers</td>
        </tr>
    }
</table>

<p><a asp-action="Index" asp-controller="Home">Return to home</a></p>